<h1>Listing photos</h1>

<table>
  <tr>
    <th>Gallery</th>
    <th>Title</th>
    <th>Body</th>
    <th>Photo url</th>
    <th>Page url</th>
    <th>Publish</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @photos.each do |photo| %>
  <tr>
    <td><%= photo.gallery_id %></td>
    <td><%= photo.title %></td>
    <td><%= photo.body %></td>
    <td><%= photo.photo_url %></td>
    <td><%= photo.page_url %></td>
    <td><%= photo.publish %></td>
    <td><%= link_to 'Show', photo %></td>
    <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
    <td><%= link_to 'Destroy', photo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Photo', new_photo_path %>
